{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Group JEAP major updates and not require an approval on the dependency dashboard for these updates",
  "packageRules": [
    {
      "matchManagers": [
        "maven"
      ],
      "matchUpdateTypes": [
        "major"
      ],
      "groupName": "JEAP (major)",
      "groupSlug": "jeap-major",
      "matchPackageNames": [
        "/^ch\\.admin\\.bit\\.jeap:/"
      ],
      "dependencyDashboardApproval": false,
      "labels": [
        "maven",
        "major",
        "jeap"
      ]
    }
  ]
}
