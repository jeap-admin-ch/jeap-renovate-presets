{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Group JEAP minor and patch updates",
  "packageRules": [
    {
      "matchManagers": [
        "maven"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "JEAP (minor & patch)",
      "groupSlug": "jeap-minor-patch",
      "matchPackageNames": [
        "/^ch\\.admin\\.bit\\.jeap:/"
      ],
      "labels": [
        "maven",
        "jeap"
      ]
    }
  ]
}
